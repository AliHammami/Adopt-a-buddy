{% extends 'base.html.twig' %}

{% block title %}Association{% endblock %}

{% block body %}
    <h1>Association</h1>

    <table class="table">
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ association.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ association.name }}</td>
            </tr>
            <tr>
                <th>Password</th>
                <td>{{ association.password }}</td>
            </tr>
            <tr>
                <th>Email</th>
                <td>{{ association.email }}</td>
            </tr>
            <tr>
                <th>Rna</th>
                <td>{{ association.rna }}</td>
            </tr>
            <tr>
                <th>Manager</th>
                <td>{{ association.manager }}</td>
            </tr>
            <tr>
                <th>Phone</th>
                <td>{{ association.phone }}</td>
            </tr>
            <tr>
                <th>Website</th>
                <td>{{ association.website }}</td>
            </tr>
            <tr>
                <th>Facebook</th>
                <td>{{ association.facebook }}</td>
            </tr>
            <tr>
                <th>Description</th>
                <td>{{ association.description }}</td>
            </tr>
            <tr>
                <th>Photo</th>
                <td>{{ association.photo }}</td>
            </tr>
            <tr>
                <th>Created_at</th>
                <td>{{ association.createdAt ? association.createdAt|date('Y-m-d H:i:s') : '' }}</td>
            </tr>
        </tbody>
    </table>

    <a href="{{ path('association_index') }}">back to list</a>

    <a href="{{ path('association_edit', {'id': association.id}) }}">edit</a>

    {{ include('association/_delete_form.html.twig') }}
{% endblock %}
